{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "chrome": "58",
            "ie": "8"
          }
        }
      ],
      ["@babel/preset-react",
         { "runtime": "automatic " }
        ],
      ["@babel/preset-typescript", {
        "isTSX": true,
        "allExtensions": true
      }]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime"
    ]
  }